{
    "name": "clowdy/laravel-raven",
    "description": "Sentry (Raven) error monitoring for Laravel with send in background",
    "keywords": ["laravel", "sentry", "getsentry", "raven", "monitoring", "error", "logging"],
    "license": "MIT",
    "authors": [
        {
            "name": "Pulkit Jalan",
            "homepage": "https://github.com/pulkitjalan"
        }
    ],
    "require": {
        "php": ">=5.4",
        "illuminate/support": "~4",
        "illuminate/queue": "~4",
        "illuminate/session": "~4",
        "illuminate/log": "~4",
        "raven/raven": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "mockery/mockery": "0.9.*"
    },
    "autoload": {
        "psr-4": {
            "Clowdy\\Raven\\": "src/"
        }
    }
}
